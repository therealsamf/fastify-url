{
  "name": "fastify-url",
  "version": "0.2.3",
  "description": "Fastify plugin for parsing and attaching URL data to requests",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test-ts": "tsd",
    "test-ci": "npm test && npm run test-ts",
    "build": "tsc",
    "lint": "eslint -c .eslintrc \"src/**/*.ts\"",
    "coverage": "nyc npm test",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "report": "nyc report --reporter=lcov",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "url",
    "fastify",
    "nodejs"
  ],
  "author": "Sam Faulkner",
  "license": "MIT",
  "types": "fastify-url.d.ts",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "eslint": "^6.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fastify": "^2.2.0",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.0.3",
    "tsd": "^0.9.0",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "fastify-plugin": "^1.5.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
